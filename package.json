{
  "name": "cellar",
  "version": "0.0.5",
  "description": "Lightweight Node.js CMS",
  "private": true,
  "keywords": [
    "cms",
    "nodejs",
    "express"
  ],
  "author": "KMD Digital",
  "maintainers": [
    {
      "name": "Keith C-R",
      "email": "keith@kmddigital.com",
      "web": ""
    }
  ],
  "homepage": "https://github.com/kmddigital/cellar",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/kmddigital/cellar.git"
    }
  ],
  "scripts": {
    "start": "node cellar.js",
    "dev": "nodemon cellar.js",
    "test": "semistandard --verbose | snazzy",
    "fix": "semistandard --fix",
    "setup": "node ./setup/setup.js"
  },
  "dependencies": {
    "async": "^2.4.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.13.2",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "dotenv-safe": "^4.0.4",
    "express": "~4.13.1",
    "express-flash": "0.0.2",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "hbs": "~3.1.0",
    "mongoose": "^4.10.5",
    "morgan": "~1.6.1",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "public-ip": "^2.3.5",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0"
  },
  "semistandard": {
    "ignore": [
      "/public/js/vendor/"
    ]
  }
}
